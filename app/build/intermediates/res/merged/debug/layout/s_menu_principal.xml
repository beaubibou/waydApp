<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@mipmap/fondwaydblancxxxhdpi"
    android:orientation="vertical"

    >

    <Button

        android:textSize="@dimen/titre1"
        android:visibility="invisible"
        android:text="@string/s_menu_principal_indicateurs"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/admin"
        android:layout_marginTop="@dimen/marge2"
        android:layout_marginLeft="@dimen/marge2"
        android:layout_marginRight="@dimen/marge2"
        android:padding="@dimen/paddingButton"/>
    <Button
        android:text="@string/s_menu_principal_ennuie"
        android:id="@+id/ennuie"
        android:background="#FFA740"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:textSize="@dimen/titre1"
        android:layout_marginTop="@dimen/marge2"
        android:layout_marginLeft="@dimen/marge2"
        android:layout_marginRight="@dimen/marge2"
        android:padding="@dimen/paddingButton"/>
    <Button
        android:padding="@dimen/paddingButton"
        android:text="@string/s_menu_principal_rechercher"
        android:layout_width="match_parent"
        android:layout_marginTop="@dimen/marge2"
        android:background="#FFC740"
        android:id="@+id/recherche"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/marge2"
        android:layout_marginRight="@dimen/marge2"
        android:textSize="@dimen/titre1"/>


    <Button
        android:id="@+id/propose"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#FFE740"
        android:padding="@dimen/paddingButton"
        android:text="@string/s_menu_principal_proposer"
        android:textSize="@dimen/titre1"
        android:layout_marginTop="@dimen/marge2"
        android:layout_marginLeft="@dimen/marge2"
        android:layout_marginRight="@dimen/marge2"

        />








</LinearLayout>
